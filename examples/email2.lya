dcl m int = 2, n int = 3;
p: proc (x, y int, b bool) returns (int);
    dcl s int = x;
    if b then
        s += y;
        result s;
    else
        result y;
    fi;
end;

dcl b bool;
read (b);
print (p(m, n, b));


